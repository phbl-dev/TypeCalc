{
    "entryPoints": ["src/"],
    "entryPointStrategy": "expand",
    "out": "docs/",
    "validation": {
        "notExported": false,
        "invalidLink": true,
        "notDocumented": true
    },
    "treatWarningsAsErrors": false,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeExternals": true
}
